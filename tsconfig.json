{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": false,
		"module": "ESNext",
		"moduleResolution": "Node",
		"outDir": "./dist/",
		"plugins": [
			{
				"transform": "typia/lib/transform"
			}
		],
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "ESNext"
	},
	"include": [
		"src/**/*.ts"
	]
}
