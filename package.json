{
	"config": {
		"region": "us-central1",
		"service_name": "breadbot"
	},
	"dependencies": {
		"@google-labs/breadboard": "0.11.2",
		"@google-labs/core-kit": "0.4.0",
		"@mermaid-js/mermaid-cli": "^10.8.0",
		"discord.js": "^14.14.1",
		"dotenv": "^16.4.4",
		"express": "^4.18.2",
		"mermaid": "^10.8.0",
		"typia": "^5.4.9"
	},
	"description": "",
	"devDependencies": {
		"@types/d3": "^7.4.3",
		"@types/dompurify": "^3.0.5",
		"@types/express": "^4.17.21",
		"@types/node": "^20.11.17",
		"nodemon": "^3.0.3",
		"puppeteer": "^22.1.0",
		"ts-node": "^10.9.2",
		"ts-patch": "^3.1.2",
		"tsx": "^4.7.1",
		"typescript": "^5.3.2"
	},
	"main": "./dist/src/index.js",
	"name": "breadbot",
	"packageManager": "yarn@4.1.0",
	"scripts": {
		"build": "tsc",
		"cloud:build": "gcloud builds submit",
		"deploy": "gcloud run deploy --source . --verbosity debug --platform managed --region $npm_package_config_region breadbot",
		"descibe:build": "gcloud builds describe $npm_package_config_service_name --region $npm_package_config_region --format export > build.yaml",
		"descibe:service": "gcloud run services describe $npm_package_config_service_name --region $npm_package_config_region --format export > service.yaml",
		"dev": "tsx watch src/index.ts",
		"gcp-build": "npm run build",
		"prebuild": "npx -y rimraf dist",
		"prepare": "ts-patch install && typia patch",
		"prestart:js": "npm run build",
		"start:js": "node dist/src/index.js",
		"start:ts": "node --loader ts-node/esm src/index.ts",
		"start:ts:watch": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm src/index.ts'"
	},
	"type": "module",
	"version": "1.0.0"
}